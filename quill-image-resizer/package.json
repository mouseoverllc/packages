{
  "name": "@mouseoverllc/quill-image-resizer",
  "version": "1.1.0",
  "author": "MouseOver",
  "description": "A lightweight plugin for Quill.js to resize images interactively.",
  "keywords": [
    "quill",
    "image",
    "resizer",
    "plugin",
    "editor",
    "typescript"
  ],
  "main": "dist/quill-image-resizer.cjs.js",
  "module": "dist/quill-image-resizer.esm.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c",
